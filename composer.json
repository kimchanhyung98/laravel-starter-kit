{
    "name": "kimchanhyung98/laravel-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "starter",
        "skeleton",
        "template"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "algolia/algoliasearch-client-php": "^3.4",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.9",
        "laravel/socialite": "^5.14",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.27",
        "predis/predis": "^2.2",
        "socialiteproviders/apple": "^5.6",
        "socialiteproviders/kakao": "^4.3"
    },
    "require-dev": {
        "dedoc/scramble": "^0.11.0",
        "fakerphp/faker": "^1.23",
        "kimchanhyung98/laravel-maker": "^0.1.1",
        "larastan/larastan": "^2.0",
        "laravel-shift/blueprint": "^2.10",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pint": "./vendor/bin/pint --preset laravel",
        "stan" : "./vendor/bin/phpstan analyse --memory-limit=2G",
        "swagger": "./vendor/bin/openapi app -o openapi.yaml -b swagger-config.php"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
